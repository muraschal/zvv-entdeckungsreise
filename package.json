    "tailwindcss-animate": "^1.0.7",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.8.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.17.0"
  },
